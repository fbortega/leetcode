var twoSum = function(nums, target) {
    const newNums = nums.filter(num => num < target)

    const solutionArray = []

    for (let i =0; i< newNums.length; i++){
        if((newNums[i] + newNums[i+1]) == target){
            solutionArray.push(i)
            solutionArray.push(i+1)
            break;
        }
    }

    return solutionArray;
};

